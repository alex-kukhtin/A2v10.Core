		<!--
	<div>
		<pre v-text="appData"></pre>
	</div>
		-->
	</div>
</div>
<script type="text/javascript">
(function () {

	'use strict';

	const eventBus = require('std:eventBus');

	const DataModelController = component('baseController');


	eventBus.$emit('beginLoad');
	const vm = new DataModelController({
		el: '#@pageGuid',
		props: {
			inDialog: { type: Boolean, default: false },
			isIndex: { type: Boolean, default: false },
			isSkipDataStack: { type: Boolean, default: true },
			pageTitle: { type: String }
		},
		data: {
			id: 0,
			appData: @Html.Raw(Model.AppData)
		},
		computed:{
			hasLogo() {
				return this.appData && this.appData.appLogo;
			},
			logoSrc() {
				return this.appData.appLogo;
			},
			locale() {
				return window.$$locale;
			}
		},
		mounted() {
			eventBus.$emit('endLoad');
		}
	});

	vm.$data._host_ = {
		$viewModel: vm,
		$ctrl: vm.__createController__(vm)
	};

	vm.__doInit__('_navpane/index/');

})();
</script>
