{
	"ConnectionStrings": {
		"Default": "Server=localhost;Database=database_name;Trusted_Connection=True;MultipleActiveResultSets=true"
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information"
		}
	},
	"AllowedHosts": "*",
	"AppSettings": {
		"GoogleMapsApiKey": "YOUR_API_KEY_HERE"
	},
	"Data": {
		"MetadataCache": false,
		"CommandTimeout": "00:20:00"
	},
	"Application": {
		"Environment": {
			"Watch": true,
			"Stage": "debug"
		},
		"MultiTenant": false,
		"MultiCompany": false,
		"Registration": true
	},
	"BlobStorage": {
		"Provider": "AzureStorage",
		"Path": "d:/temp/FileSystemStorage"
	},
	"MailSettings": {
		"Host": "smtp.gmail.com",
		"Port": 587,
		"From": "a2v10@app.mail",
		"UserName": "user_name",
		"Password": "user_password",
		"Secure": "Auto",
		"SkipCertificateValidation": false,
		"DeliveryMethod": "FileSystem",
		"PickupDirectoryLocation": "c:/temp"
	},

	"Globalization": {
		"Locale": "uk-UA",
		"DateLocale": "uk-UA",
		"NumberLocale": "uk-UA",
		"AvailableLocales2": [ "uk-UA", "en-US" ],
		"AvailableLocales": "uk-UA,en-US,ru-RU"
	},

	"Scheduler2": {
		"Jobs": [
			{
				"Id": "Test1",
				"Handler": "A2v10.Scheduling.ExecuteSqlJobHandler",
				"Cron": "0 * * ? * *", /* every minute */
				"DataSource": "Default",
				"Procedure": "dbo.[DoPending]",
				"Parameters": {
					"Number": 5,
					"Boolean": true,
					"String": "string 1"
				}
			},
			{
				"Id": "Test2",
				"Handler": "ExecuteSql",
				"Cron": "0 * * ? * *", /* every minute */
				"DataSource": "Default",
				"Procedure": "dbo.[DoPending]",
				"Parameters": {
					"Number": 5,
					"Boolean": true,
					"String": "string 1"
				}
			},
			{
				"Id": "CommandQueue",
				"Handler": "ProcessCommands",
				"Cron": "20 * * ? * *", /* every minute */
				"DataSource": "Default"
			}
		]
	}
}
