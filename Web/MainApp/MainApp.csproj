<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<LangVersion>Latest</LangVersion>
		<OutputType>Library</OutputType>
		<NoWarn>$(NoWarn);MSB3245</NoWarn>
		<NoWin32Manifest>true</NoWin32Manifest>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="A2v10.App.Assets" Version="10.1.8620" />
		<PackageReference Include="A2v10.Data.Interfaces" Version="10.1.7583" />
		<PackageReference Include="A2v10.Sql.MSBuild" Version="10.1.8200" />
		<PackageReference Include="Microsoft.TypeScript.MSBuild" Version="5.9.3">
			<PrivateAssets>all</PrivateAssets>
			<IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
	</ItemGroup>

	<ItemGroup>
		<Folder Include="_Global\" />
		<Folder Include="_sqlscripts\" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\..\Platform\A2v10.App.Infrastructure\A2v10.App.Infrastructure.csproj" />
	</ItemGroup>

</Project>
