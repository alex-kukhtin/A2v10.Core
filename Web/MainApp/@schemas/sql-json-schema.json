	"$version": "10.0.7644",
	"title": "JSON Schema for sql.json",
	"type": "array",
	"items": {
		"type": "object",
		"properties": {
			"$schema": { "type": "string" },
			"name": { "type": "string" },
			"version": { "type": "string" },
			"outputFile": { "type": "string" },
			"inputFiles": {
				"type": "array",
				"items": { "type": "string" }
