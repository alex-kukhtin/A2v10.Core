<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
	  <LangVersion>10.0</LangVersion>
	  <Nullable>enable</Nullable>
	  <AssemblyName>A2v10.Xaml.Report</AssemblyName>
	  <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	  <Version>10.1.8085</Version>
	  <Product>A2v10</Product>
	  <PackageId>A2v10.Xaml.Report</PackageId>
	  <Authors>Oleksandr Kukhtin</Authors>
	  <Company>Oleksandr Kukhtin</Company>
	  <Description>PDF reports for A2v10</Description>
	  <Copyright>Copyright Â© 2020-2022 Oleksandr Kukhtin. All rights reserved.</Copyright>
	  <RepositoryType>git</RepositoryType>
	  <RepositoryUrl>https://github.com/alex-kukhtin/A2v10.Core</RepositoryUrl>
	  <PackageTags>A2v10 Reports Xaml Pdf</PackageTags>
  </PropertyGroup>

	<PropertyGroup>
		<ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
		<EmbedUntrackedSources>true</EmbedUntrackedSources>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<NeutralLanguage>en</NeutralLanguage>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.1.1" PrivateAssets="All" />
	</ItemGroup>

	<ItemGroup>
    <PackageReference Include="A2v10.System.Xaml" Version="10.1.8016" />
  </ItemGroup>

</Project>
