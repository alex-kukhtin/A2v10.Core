{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"AllowedHosts": "*",
	"Authentication": {
		"JwtBearer": {
			"Issuer": "token issuer",
			"Audience": "token Audience",
			"ExpireMinutes": 10080,
			"SecurityKey": "<SecurityKeyValue> (min 16 chars)"
		}
	}
}
